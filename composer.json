{
    "name": "51systems/doctrine-encrypt",
    "type": "library",
    "keywords": ["doctrine", "aes256", "encrypt", "decrypt"],
    "license": "MIT",
    "description": "Package encrypts and decrypts Doctrine fields through life cycle events.",
    "require": {
        "php": "^7.0",
        "doctrine/orm": "^2.5"
    },
    "require-dev": {
        "illuminate/encryption": "^5.4",
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "DoctrineEncrypt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DoctrineEncrypt\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Victor Melnik",
            "email": "melnikvictorl@gmail.com"
        },
        {
            "name": "Dustin Thomson",
            "email": "dthomson@51systems.com"
        },
        {
            "name": "Dries Vints",
            "email": "dries.vints@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true
    }
}
